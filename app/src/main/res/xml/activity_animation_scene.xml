<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyPosition
               motion:motionTarget="@+id/airplane1_id"
               motion:framePosition="30"
               motion:keyPositionType="deltaRelative"
               motion:percentX="0.419"
               motion:percentY="0.832" />
           <KeyPosition
               motion:motionTarget="@+id/airplane1_id"
               motion:framePosition="10"
               motion:keyPositionType="deltaRelative" />
       </KeyFrameSet>
        <OnClick motion:targetId="@+id/airplane1_id" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="104dp"
            motion:layout_editor_absoluteX="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="528dp"
            android:layout_width="170dp"
            android:id="@+id/cloud_id" />
        <Constraint
            android:layout_height="55dp"
            motion:layout_editor_absoluteX="-29dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="228dp"
            android:layout_width="83dp"
            android:id="@+id/airplane1_id" />
        <Constraint
            android:layout_height="76dp"
            motion:layout_editor_absoluteX="267dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="116dp"
            android:layout_width="120dp"
            android:id="@+id/cloud3_id" />
        <Constraint
            android:layout_height="76dp"
            motion:layout_editor_absoluteX="245dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="120dp"
            android:id="@+id/cloud2_id"
            android:layout_marginBottom="284dp" />
        <Constraint
            motion:layout_editor_absoluteY="16dp"
            android:layout_height="20dp"
            android:layout_width="wrap_content"
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp" />
        <Constraint
            android:layout_height="76dp"
            motion:layout_editor_absoluteX="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="120dp"
            android:id="@+id/airplane_id" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="675dp"
            android:id="@+id/textView3"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="259dp" />
        <Constraint
            android:layout_height="103dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="112dp"
            android:layout_width="163dp"
            android:id="@+id/cloud_id"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp" />
        <Constraint
            android:layout_height="29dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="220dp"
            android:layout_width="77dp"
            android:id="@+id/cloud2_id"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="92dp"
            android:layout_marginStart="92dp" />
        <Constraint
            android:layout_height="116dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="384dp"
            android:layout_width="163dp"
            android:id="@+id/cloud3_id"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="36dp"
            android:layout_marginStart="36dp" />
        <Constraint
            android:layout_height="52dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="396dp"
            android:layout_width="71dp"
            android:id="@+id/airplane1_id"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp" />
    </ConstraintSet>
</MotionScene>